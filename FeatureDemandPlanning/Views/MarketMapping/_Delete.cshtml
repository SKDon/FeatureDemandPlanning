@using FeatureDemandPlanning.Model.Extensions

@model FeatureDemandPlanning.Model.ViewModel.MarketMappingViewModel

<form>
    <input id="Action_IdentifierPrefix" type="hidden" value="@Model.IdentifierPrefix" />
</form>
<div class="well col-xs-12">
    <div class="col-xs-4 text-right">
        <strong>Import Market:</strong>
    </div>
    <div class="col-xs-8">
        @Model.MarketMapping.ImportMarket
    </div>
    <div class="col-xs-4 text-right">
        <strong>Programme:</strong>
    </div>
    <div class="col-xs-8">
        @Model.MarketMapping.Programme.GetDisplayString()
    </div>
    <div class="col-xs-4 text-right" style="margin-bottom:15px">
        <strong>Gateway:</strong>
    </div>
    <div class="col-xs-8" style="margin-bottom:15px">
        @Model.MarketMapping.Gateway
    </div>
    <div class="col-xs-4 text-right">
        <strong>Mapped Market:</strong>
    </div>
    <div class="col-xs-8">
        @Model.MarketMapping.Name
    </div>
 </div>
