@using enums = FeatureDemandPlanning.Model.Enumerations

@model FeatureDemandPlanning.Model.ViewModel.FeatureMappingViewModel

<div class="panel panel-primary">
    <div class="panel-heading" style="height:38px">
        <h3 class="panel-title results-paging pull-right"></h3>
        <h3 class="panel-title results-total pull-left">Features Codes</h3>
    </div>
    <div class="panel-body">
        <table id="@(Model.IdentifierPrefix)_FeatureCodeTable" class="table table-hover table-striped table-bordered dataTable">
            <thead>
            <tr>
                <th></th>
                <th>Programme</th>
                <th class="text-center">Gateway</th>
                <th class="center">Document</th>
                <th class="text-center">Feature Code</th>
                <th class="text-center">Description</th>
            </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>
