@model FeatureDemandPlanning.Model.ViewModel.FeatureMappingViewModel

<script language="javascript">

    var featureMapping, page, modal;
    var params = {
        ActionsUri: "@Url.Action("ContextMenu")",
        ProcessUri: "@Url.Action("Process")",
        CancelUri: "@Url.Action("Cancel")",
        FeatureMappingsUri: "@Url.Action("ListFeatureMappings")",
        ModalContentUri: "@Url.Action("ModalContent")",
        ModalActionUri: "@Url.Action("ModalAction")",
        ModalDialogId: "modal",
        ModalContentId: "modalBody",
        Configuration: {},
        PageSize: @Model.PageSize,
        PageIndex: @Model.PageIndex,
        _lastParam: null
    };
    params.Configuration = @Html.Raw(Json.Encode(Model.Configuration));

</script>

@Scripts.Render("~/bundles/js/shared")
@Scripts.Render("~/bundles/js/featuremapping")
<script language="javascript" src="@Url.Content("~/Content/js/FeatureMapping/init.js")?v=@Model.CurrentVersion"></script>