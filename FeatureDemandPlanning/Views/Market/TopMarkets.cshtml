@using FeatureDemandPlanning.Model;
@using enums = FeatureDemandPlanning.Model.Enumerations;

@model FeatureDemandPlanning.Model.ViewModel.MarketViewModel

<div class="col-lg-12">
        @foreach (var market in Model.TopMarkets)
        {
            <div class="col-lg-3" style="padding-bottom:8px">

                <div class="label label-primary" style="font-weight:normal;display:block;text-align:left;padding-top:5px">
                    <a href="javascript:markets.deleteMarket(@market.Id)">
                        <i class="glyphicon glyphicon-trash glyphicon"></i>
                    </a>
                    @market.Name
                </div>
            </div>
        }
</div>





