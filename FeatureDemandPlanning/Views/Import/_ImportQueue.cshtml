@using enums = FeatureDemandPlanning.Model.Enumerations

@model FeatureDemandPlanning.Model.ViewModel.ImportViewModel

<div class="panel panel-primary">
    <div class="panel-heading" style="height:38px">
        <h3 class="panel-title results-paging pull-right"></h3>
        <h3 class="panel-title results-total pull-left">Import Items</h3>
    </div>
    <div class="panel-body">

        <table id="tblImportQueue" class="table table-hover table-striped table-bordered dataTable">
            <thead>
                <tr>
                    <th class="text-center">Uploaded On</th>
                    <th>Uploaded By</th>
                    <th>Vehicle</th>
                    <th>File</th>
                    <th class="text-center">Status</th>
                    <th class="text-center">Errors</th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>